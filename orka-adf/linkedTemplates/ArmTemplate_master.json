{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "orka-adf"
		},
		"ADLS_orka_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_orka'"
		},
		"ASQL_orka_Entertainment_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ASQL_orka_Entertainment'"
		},
		"ADLS_orka_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://orkadatalake.dfs.core.windows.net"
		},
		"HTTP_Rebrickable_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"SensorTower_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.sensortower.com/v1/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "orka-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_orka_accountKey": {
						"value": "[parameters('ADLS_orka_accountKey')]"
					},
					"ASQL_orka_Entertainment_connectionString": {
						"value": "[parameters('ASQL_orka_Entertainment_connectionString')]"
					},
					"ADLS_orka_properties_typeProperties_url": {
						"value": "[parameters('ADLS_orka_properties_typeProperties_url')]"
					},
					"HTTP_Rebrickable_properties_typeProperties_url": {
						"value": "[parameters('HTTP_Rebrickable_properties_typeProperties_url')]"
					},
					"SensorTower_properties_typeProperties_url": {
						"value": "[parameters('SensorTower_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}